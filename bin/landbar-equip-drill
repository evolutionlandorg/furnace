#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

landId=0x2a0100010100010100000000000000010000000000000000000000000000011e
index=$(seth --to-uint256 $1)
token=$IOBJECTOWNERSHIP_PROXY
id=0x2a0100010100010500000000000000010000000000000000000000000000000a
# seth send $IOBJECTOWNERSHIP_PROXY "setApprovalForAll(address,bool)" $LANDITEMBAR_PROXY $(seth --to-uint256 1)
seth send $LANDITEMBAR_PROXY "equip(uint256,uint256,address,uint256)" $landId $index $token $id
