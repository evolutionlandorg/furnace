#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

path=$FNC_CONFIG
address=$(dapp create src/FormulaInit.sol:FormulaInit $ISETTINGSREGISTRY)

authAddress=$(dapp create src/FormulaAuthority.sol:FormulaAuthority [$address])
seth call $authAddress "whiteList(address)" $address  
seth send $FORMULA_PROXY "setAuthority(address)" $authAddress

seth send $address "initFormula()"
