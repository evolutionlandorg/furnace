#!/usr/bin/env bash

set -e

if [ "$FNC_VERBOSE" ]; then set -x; fi

. $(PWD)/bin/init

path=$FNC_CONFIG
address=$(dapp create src/ItemLuckyBox.sol:ItemLuckyBox $ISETTINGSREGISTRY $ITEMTAKEBACK $(seth --to-uint256 1605657600))
export ITEMLUCKYBOX=$address
seth call $ITEMLUCKYBOX "getPrice()(uint256,uint256)"   # 1000 100  
cat $path | jq '.ITEMLUCKYBOX = $address' --arg address $ITEMLUCKYBOX | sponge $path
