#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

apostleId=0x2a0100010100010200000000000000010000000000000000000000000000006b
landId=0x2a0100010100010100000000000000010000000000000000000000000000011e

seth send $LANDRESOURCE_PROXY "startMining(uint256,uint256,address)" $apostleId $landId $TOKEN_GOLD  
