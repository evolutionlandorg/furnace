#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

landId=0x2a0100010100010100000000000000010000000000000000000000000000011e
index=$(seth --to-uint256 $1)
seth send $LANDRESOURCE_PROXY "startMining(uint256,uint256,uint256,address)" $index $2 $landId $3  
# seth send $LANDRESOURCE_PROXY "startMining(uint256,uint256,address)" $1 $landId $2  
