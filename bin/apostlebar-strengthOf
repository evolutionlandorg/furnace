#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

apostleId=0x2a0100010100010200000000000000010000000000000000000000000000006b

tokens=($TOKEN_GOLD $TOKEN_WOOD $TOKEN_WATER $TOKEN_FIRE $TOKEN_SOIL)
for token in "${tokens[@]}"; do
	seth call $APOSTLEITEMBAR_PROXY "enhanceStrengthRateOf(address,uint256)(uint256)" $token $apostleId
done
