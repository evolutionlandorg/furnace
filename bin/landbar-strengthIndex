#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

landId=0x2a0100010100010100000000000000010000000000000000000000000000011e

for i in {0..4}; do
	index=$(seth --to-uint256 $i)
	seth call $LANDITEMBAR_PROXY "enhanceStrengthRateByIndex(address,uint256,uint256)(uint256)" $TOKEN_GOLD $landId $index		
	seth estimate $LANDITEMBAR_PROXY "enhanceStrengthRateByIndex(address,uint256,uint256)(uint256)" $TOKEN_GOLD $landId $index		
done

