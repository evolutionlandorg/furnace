#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

seth call $METADATATELLER_PROXY  "getRate(address,uint256,uint256)(uint256)" $IOBJECTOWNERSHIP_PROXY 0x2a010001010001050000000000000001000000000000000000000000000003f4 $(seth --to-uint256 $1)
seth estimate $METADATATELLER_PROXY  "getRate(address,uint256,uint256)(uint256)" $IOBJECTOWNERSHIP_PROXY 0x2a010001010001050000000000000001000000000000000000000000000003f4 $(seth --to-uint256 $1)
