#!/usr/bin/env bash

set -e

. $(PWD)/bin/init


for i in {0..4}; do
	index=$(seth --to-uint256 $i)
	seth call $LANDITEMBAR_PROXY "enhanceStrengthRateByIndex(address,uint256,uint256)(uint256)" $TOKEN_GOLD $1 $index		
	seth estimate $LANDITEMBAR_PROXY "enhanceStrengthRateByIndex(address,uint256,uint256)(uint256)" $TOKEN_GOLD $1 $index		
done

